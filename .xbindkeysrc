#Increase volume
"pactl set-sink-volume @DEFAULT_SINK@ +1000 ; kill -44 $(pidof dwmblocks)"
   XF86AudioRaiseVolume

# Decrease volume
"pactl set-sink-volume @DEFAULT_SINK@ -1000 ; kill -44 $(pidof dwmblocks)"
   XF86AudioLowerVolume

# Mute volume
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
   XF86AudioMute

# Play and pause audio
"playerctl play-pause"
   XF86AudioPlay

# Skip track
 "playerctl next"
   XF86AudioNext

# Earlier track
"playerctl previous"
   XF86AudioPrev
